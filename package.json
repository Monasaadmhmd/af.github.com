{
  "name": "af.github.com",
  "version": "0.0.0",
  "description": "Silly little personal website",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "eslint js",
    "prepush": "npm run lint",
    "serve": "jekyll serve --watch",
    "start": "concurrently \"npm run serve\" \"npm run watch\" --names jekyll,webpack -c blue,gray.dim",
    "watch": "webpack -w"
