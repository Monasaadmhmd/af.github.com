{
  "name": "af.github.com",
  "version": "0.0.0",
  "description": "Silly little personal website",
  "scripts": {
    "analyze": "ANALYZE_BUNDLE=1 NODE_ENV=production webpack",
    "build": "rm -rf _site/* && eleventy && NODE_ENV=production webpack",
    "deploy": "yarn build && netlify deploy",
    "deploy-prod": "yarn build && netlify deploy --prod",
    "lint": "eslint js",
    "start": "concurrently \"eleventy --quiet --serve\" \"webpack -w\" --names 11ty,wbpk -c blue,gray.dim",
    "watch": "yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/af/af.github.com.git"
  },
  "author": "Aaron Franks (http://aaronfranks.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/af/af.github.com/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.10.0",
    "@11ty/eleventy-plugin-rss": "1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1",
    "concurrently": "5.1.0",
    "css-loader": "3.4.0",
    "eslint": "6.8.0",
    "eslint-config-af": "af/eslint-config-af#master",
    "husky": "4.2.5",
    "mini-css-extract-plugin": "0.9.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "terser-webpack-plugin": "2.3.5",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {
    "d3": "5.15.1",
    "moment": "2.24.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  }
}
