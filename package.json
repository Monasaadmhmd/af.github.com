{
  "name": "af.github.com",
  "version": "0.0.0",
  "description": "Silly little personal website",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "lint": "eslint js",
    "prepush": "npm run lint",
    "serve": "bundle exec jekyll serve",
    "start": "concurrently \"npm run serve\" \"npm run watch\" --names jekyll,webpack -c blue,gray.dim",
    "watch": "webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/af/af.github.com.git"
  },
  "author": "Aaron Franks (http://aaronfranks.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/af/af.github.com/issues"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "eslint": "5.16.0",
    "eslint-config-af": "af/eslint-config-af#master",
    "husky": "0.14.3",
    "stylus": "0.54.5",
    "terser-webpack-plugin": "1.3.0",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4"
  },
  "dependencies": {
    "d3": "5.9.2",
    "stylepack": "0.5.3"
  }
}
